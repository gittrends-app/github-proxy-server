{
  "name": "@gittrends-app/github-proxy-server",
  "version": "6.1.0",
  "main": "./dist/middleware.js",
  "types": "./dist/middleware.d.ts",
  "repository": "git@github.com:gittrends-app/github-proxy-server.git",
  "author": "Hudson Silva Borges <hudsonsilbor@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/cli.js",
    "dev": "ts-node-dev --respawn --transpile-only src/cli.ts",
    "dev-no-reload": "ts-node --transpile-only src/cli.ts",
    "test": "jest --no-cache --coverage --forceExit",
    "test:detectOpenHandles": "jest --no-cache --coverage --runInBand --detectOpenHandles",
    "commit": "git-cz",
    "lint": "eslint src --ext .ts",
    "build": "rm -rf dist/* && tsc && npm run add-version",
    "pkg": "npm run build && pkg . && chmod +x bin/*",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && git add dist",
    "prettier": "prettier --write --config .prettierrc.yml --ignore-path .prettierignore  '**/*.ts'",
    "version": "node -e 'console.log(process.env.npm_package_version)'",
    "add-version": "echo \"npm_package_version=$(node -e 'console.log(process.env.npm_package_version)')\" > dist/.env.version"
  },
  "bin": {
    "github-proxy-server": "./dist/cli.js"
  },
  "np": {
    "yarn": true,
    "tests": true
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist",
    "architecture.png"
  ],
  "dependencies": {
    "bottleneck": "^2.19.5",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv-override-true": "^6.2.2",
    "express": "^4.17.1",
    "express-status-monitor": "^1.3.3",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "http-proxy": "^1.18.1",
    "ioredis": "^4.28.0",
    "ip": "^1.1.5",
    "lodash": "^4.17.21",
    "response-time": "^2.3.2",
    "table": "^6.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@jest/globals": "^27.3.1",
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/async": "^3.2.9",
    "@types/compression": "^1.7.2",
    "@types/connect-timeout": "^0.0.35",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-status-monitor": "^1.2.4",
    "@types/faker": "^5.5.9",
    "@types/http-proxy": "^1.17.7",
    "@types/ip": "^1.1.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@types/response-time": "^2.3.5",
    "@types/table": "^6.3.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "axios": "^0.24.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "get-port": "^5.1.1",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "nock": "^13.2.0",
    "np": "^7.5.0",
    "pkg": "^5.4.1",
    "prettier": "^2.4.1",
    "tmp-promise": "^3.0.3",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pkg": {
    "assets": "dist/.env.version",
    "outputPath": "bin"
  }
}
