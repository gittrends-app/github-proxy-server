{
  "name": "@gittrends-app/github-proxy-server",
  "version": "7.3.0",
  "main": "./dist/middleware.js",
  "types": "./dist/middleware.d.ts",
  "repository": "git@github.com:gittrends-app/github-proxy-server.git",
  "author": "Hudson Silva Borges <hudsonsilbor@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/cli.js",
    "dev": "ts-node-dev --respawn --transpile-only src/cli.ts",
    "dev-no-reload": "ts-node --transpile-only src/cli.ts",
    "test": "jest --no-cache --coverage --forceExit",
    "test:detectOpenHandles": "jest --no-cache --coverage --runInBand --detectOpenHandles",
    "commit": "git-cz",
    "lint": "eslint src --ext .ts",
    "build": "rm -rf dist/* && tsc && npm run add-version",
    "pkg": "npm run build && pkg . && chmod +x bin/*",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\"",
    "prettier": "prettier --write --config .prettierrc.yml --ignore-path .prettierignore  '**/*.ts'",
    "version": "node -e 'console.log(process.env.npm_package_version)'",
    "add-version": "echo \"npm_package_version=$(node -e 'console.log(process.env.npm_package_version)')\" > dist/.env.version",
    "release": "standard-version"
  },
  "bin": {
    "github-proxy-server": "./dist/cli.js"
  },
  "np": {
    "yarn": true,
    "tests": true,
    "message": "chore: v%s"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist",
    "architecture.png"
  ],
  "dependencies": {
    "@fastify/express": "^1.1.0",
    "bottleneck": "^2.19.5",
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.2",
    "dotenv-override-true": "^6.2.2",
    "express-status-monitor": "^1.3.4",
    "fastify": "^3.29.0",
    "fastify-formbody": "^5.3.0",
    "helmet": "^5.0.2",
    "http-proxy": "^1.18.1",
    "ioredis": "^5.0.4",
    "ip": "^1.1.7",
    "lodash": "^4.17.21",
    "response-time": "^2.3.2",
    "table": "^6.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@jest/globals": "^28.1.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/async": "^3.2.13",
    "@types/compression": "^1.7.2",
    "@types/connect-timeout": "^0.0.36",
    "@types/cors": "^2.8.12",
    "@types/express-status-monitor": "^1.2.4",
    "@types/faker": "^5.5.9",
    "@types/http-proxy": "^1.17.9",
    "@types/ip": "^1.1.0",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.32",
    "@types/response-time": "^2.3.5",
    "@types/table": "^6.3.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "axios": "^0.27.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "get-port": "^5.1.1",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "leaked-handles": "^5.2.0",
    "nock": "^13.2.4",
    "np": "^7.6.3",
    "pkg": "^5.6.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.3.2",
    "tmp-promise": "^3.0.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pkg": {
    "assets": "dist/.env.version",
    "outputPath": "bin"
  }
}
