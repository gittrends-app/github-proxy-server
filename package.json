{
  "name": "@hsborges/github-proxy-server",
  "version": "5.0.1",
  "main": "./dist/middleware.js",
  "types": "./dist/middleware.d.ts",
  "repository": "git@github.com:hsborges/github-proxy-server.git",
  "author": "Hudson Silva Borges <hudsonsilbor@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/cli.js",
    "dev": "ts-node-dev --respawn --transpile-only src/cli.ts",
    "test": "jest",
    "commit": "git-cz",
    "lint": "eslint . --ext .ts",
    "build": "rm -rf dist/* && tsc && npm run add-version",
    "pkg": "npm run build && pkg .",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write --config .prettierrc.yml --ignore-path .prettierignore  '**/*.ts'",
    "version": "node -e 'console.log(process.env.npm_package_version)'",
    "add-version": "echo \"npm_package_version=$(node -e 'console.log(process.env.npm_package_version)')\" > dist/.env.version"
  },
  "bin": {
    "github-proxy-server": "./dist/cli.js"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "np": {
    "yarn": true,
    "tests": true
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "bottleneck": "^2.19.5",
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv-override-true": "^6.2.2",
    "express": "^4.17.1",
    "express-status-monitor": "^1.3.3",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "http-proxy": "^1.18.1",
    "ioredis": "^4.28.0",
    "ip": "^1.1.5",
    "lodash": "^4.17.21",
    "response-time": "^2.3.2",
    "table": "^6.7.1"
  },
  "devDependencies": {
    "@jest/globals": "^27.1.1",
    "@trivago/prettier-plugin-sort-imports": "^3.1.0",
    "@types/async": "^3.2.7",
    "@types/compression": "^1.7.2",
    "@types/connect-timeout": "^0.0.35",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-status-monitor": "^1.2.4",
    "@types/faker": "^5.5.9",
    "@types/http-proxy": "^1.17.7",
    "@types/ip": "^1.1.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/response-time": "^2.3.5",
    "@types/table": "^6.3.2",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "axios": "^0.21.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "get-port": "^5.1.1",
    "jest": "^27.1.1",
    "np": "^7.5.0",
    "pkg": "^5.4.1",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pkg": {
    "assets": "dist/.env.version",
    "outputPath": "bin"
  }
}
